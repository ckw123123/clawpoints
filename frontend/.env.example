# Production Environment Configuration
# Copy this file to .env.production and fill in the actual values

# API Configuration
REACT_APP_API_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_STAGE=prod
REACT_APP_REGION=us-east-1

# Cognito Configuration
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
REACT_APP_COGNITO_CLIENT_ID=your-cognito-client-id

# Optional: Analytics and Monitoring
REACT_APP_ANALYTICS_ID=your-analytics-id
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Feature Flags
REACT_APP_ENABLE_CAMERA_SCANNING=true
REACT_APP_ENABLE_CHATBOT=true
REACT_APP_ENABLE_SOCIAL_INTEGRATION=true